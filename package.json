{
  "name": "server",
  "version": "1.0.0",
  "description": "Sports Event Platform Backend",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "app": "nodemon app.js",
    "dev": "nodemon src/index.js",
    "dev-app": "nodemon app.js",
    "build": "echo 'No build step required'",
    "migrate": "sequelize-cli db:migrate --env production",
    "migrate:dev": "sequelize-cli db:migrate",
    "migrate:undo": "sequelize-cli db:migrate:undo",
    "seed": "sequelize-cli db:seed:all --env production",
    "seed:dev": "sequelize-cli db:seed:all",
    "db:create": "sequelize-cli db:create",
    "db:drop": "sequelize-cli db:drop",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "sports",
    "events",
    "api"
  ],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "openai": "^5.16.0",
    "pg": "^8.16.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.7",
    "socket.io": "^4.8.1"
  },
  "devDependencies": {
    "nodemon": "^3.1.10",
    "sequelize-cli": "^6.6.3"
  }
}
